{
  "name": "forecast",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "start": "run-p -lns start:client start:server",
    "start:client": "react-scripts start",
    "start:server": "tsnd --respawn -P tsconfig.server.json src/api/index.ts",
    "sc": "npm run start:client",
    "ss": "npm run start:server",
    "ss:dev": "cross-env NODE_ENV=development npm run ss",
    "ss:prod": "cross-env NODE_ENV=production npm run ss",
    "ss:test": "cross-env NODE_ENV=test npm run ss",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "run-s -c lint:js lint:css",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:css": "stylelint src/**/*.{css,scss,sass,less,js,jsx,ts,tsx}",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "dark-sky": "^1.1.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "i18next": "^19.0.3",
    "i18next-browser-languagedetector": "^4.0.1",
    "localforage": "^1.7.3",
    "luxon": "^1.21.3",
    "normalize.css": "^8.0.1",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-collapsible": "^2.6.2",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.7",
    "react-md-spinner": "^1.0.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.3.0",
    "react-slick": "^0.25.2",
    "react-toastify": "^5.5.0",
    "react-use": "^13.14.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.4.1",
    "url": "^0.11.0",
    "weathericons": "^2.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/luxon": "^1.21.0",
    "@types/node": "^13.1.6",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.17",
    "@types/react-beautiful-dnd": "^11.0.4",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-slick": "^0.23.4",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-plugin-macros": "^2.8.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-jest-enzyme": "^7.1.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "fetch-mock": "^8.3.1",
    "husky": "^4.0.4",
    "lint-staged": "^9.5.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.{js,ts}",
      "!src/serviceWorker.{js,ts}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  }
}
