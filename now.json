{
  "builds": [{
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "src/api/index.ts",
      "use": "@now/node-server"
    }
  ],
  "routes": [{
    "src": "^/api(.*)",
    "dest": "/src/api/index.ts"
  }]
}
